{
  "name": "vue-running",
  "version": "0.0.0",
  "private": true,
  "description": "",
  "keywords": [],
  "homepage": "https://github.com/estjs/vruning#readme",
  "bugs": {
    "url": "https://github.com/estjs/vue-running/issues"
  },
  "license": "MIT",
  "author": "jiangxd <jangxd2016@gmail.com>",
  "files": [
    "dist"
  ],
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    },
    "./style.css": "./dist/style.css",
    "./dist/style.css": "./dist/style.css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/estjs/vue-running.git"
  },
  "types": "./dist/index.d.ts",
  "scripts": {
    "dev": "rollup -c scripts/rollup.config.js  -w ",
    "build": "vite build",
    "build-types": "vue-tsc -p ./tsconfig.build.json --declaration --emitDeclarationOnly",
    "play": "vite playground --open",
    "types":" && api-extractor run -c api-extractor.json ",
    "play:build": "vite build playground",
    "lint": "eslint . --fix",
    "test:e2e": "cypress open",
    "prepublishOnly": "pnpm run build",
    "release": "bumpp --commit --tag --push && pnpm publish"
  },

  "devDependencies": {
    "@estjs/eslint-config": "^0.2.21",
    "@microsoft/api-extractor": "^7.19.2",
    "@rollup/plugin-commonjs": "^22.0.0",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@types/codemirror": "^5.60.5",
    "@types/node": "^17.0.31",
    "@vitejs/plugin-vue": "^2.3.2",
    "bumpp": "^7.1.1",
    "codemirror": "^5.65.3",
    "rimraf": "^3.0.2",
    "sucrase": "^3.21.0",
    "typescript": "^4.5.4",
    "vite": "^2.6.5",
    "vue": "^3.2.19",
    "vue-tsc": "^0.29.8"
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  }
}
